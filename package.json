{
  "name": "schooler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "node db/seed.js",
    "db:setup": "psql $DATABASE_URL -f db/schema.sql && npm run db:seed",
